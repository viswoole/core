{
  "name": "viswoole/core",
  "description": "这是ViSwoole的核心构建库",
  "keywords": [
    "framework",
    "core",
    "viswoole"
  ],
  "minimum-stability": "dev",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "zhuchonglin",
      "email": "8210856@qq.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-swoole": ">=5.1",
    "psr/container": ">=2.0.2",
    "symfony/console": ">=v6.3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "files": [
      "src/helper.php"
    ],
    "psr-4": {
      "ViSwoole\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ViSwoole\\Core\\Tests\\": "tests"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "cd ../../.. && php viswoole service:discover"
    ],
    "post-update-cmd": [
      "@php pwd"
    ]
  }
}
